student_scores = [150, 142, 185, 120, 171, 184, 149, 24, 59, 68, 199, 78, 65, 89, 86, 55, 91, 64, 89]

# calculate sum of the above list
sum = 0
for score in student_scores:
    sum+=score
print(sum)

#Calculating max
max_num = student_scores[0]

for score in student_scores:
    if score > max_num:
        max_num = score
    else:
        continue

print(max_num)
print(max(student_scores))
